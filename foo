b={1,0 2,3 3,0 4,-1 5,8}	/* define b */

c= {1,1 2,2 3,3			/* demonstrate line wrapping */
    4,5 6,8}

a={1,9 2,0 3,-1 4,6 5,5}	/* define a */
d=warp(a,0.1)		        /* delay a by 0.1 */
e=a-d				/* take partial derivative of d */

x={0,-1 1,1 2,1 3,-1 4,-1 5,1 6,1}

